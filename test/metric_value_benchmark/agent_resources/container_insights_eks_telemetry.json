{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "EMF structured log schema",
  "description": "JSON schema for the CloudWatch agent structured log",
  "type": "object",
  "properties": {
    "AutoScalingGroupName": {},
    "CloudWatchMetrics": {
      "type": "array",
      "items": {
        "Namespace": {
          "type": "string"
        },
        "Dimensions": {
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "Metrics": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
              "Name": {
                "type": "string"
              },
              "Unit": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "ClusterName": {},
    "InstanceId": {},
    "InstanceType": {},
    "Namespace": {},
    "NodeName": {},
    "PodName": {},
    "Sources": {
      "type": "array"
    },
    "Timestamp": {},
    "Type": {},
    "Version": {},
    "kubernetes": {
      "type": "array"
    }
  },
  "required": [
    "AutoScalingGroupName",
    "ClusterName",
    "InstanceId",
    "InstanceType",
    "Namespace",
    "NodeName",
    "PodName",
    "Service",
    "Sources",
    "Timestamp",
    "Type",
    "Version",
    "kubernetes"
  ],
  "additionalProperties": true
}